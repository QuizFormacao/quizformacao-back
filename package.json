    "build": "tsc --project ./ && npm run doc",
    "doc": "node build/src/SwaggerGenerate.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "test": "ts-node tests/Setup.ts",
    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix"
  },
  "dependencies": {
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.45"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.6",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.1",
    "jest": "^27.5.1",
    "nodemon": "^2.0.12",
    "swagger-autogen": "^2.19.0",
    "supertest": "^6.2.2",
    "sqlite3": "^5.0.8",
    "ts-node": "^10.7.0",
    "ts-jest": "^27.1.3",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "^4.6.2"
  }
